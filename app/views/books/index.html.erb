<h2><strong>Books<strong></h2>
<% @books.each do |book| %>
<h4>Title</h4>
<%= book.title%>

<h4>Body</h4>
<%= book.body%>

<% end %>>
<!--linktoが３つ入りそう-->

<h2><strong>New book</strong></h2>
<%= form_with model:@book, model:@book, local:true do |f| %>

<h4>Title</h4>
<%= f.text_field :title %>

<h4>Body</h4>
<%= f.text_area :body %>

<br>
<%= f.submit 'Create books' %>

<% end %>
